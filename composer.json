{
    "name": "elephant/status",
    "type": "library",
    "description" : "Check the response code of the page. Checking for 404 errors on the site.",
    "keywords": [
        "page-http-code",
        "not-found",
        "404",
        "404 error"
    ],
    "homepage": "https://github.com/pavelkorshunov/elephant-status",
    "authors": [
        {
            "name": "Pavel Korshunov",
            "email": "info@hard-skills.ru"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-dom": "*",
        "ext-mbstring": "*",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "^7.4",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Required for http requests"
    },
    "autoload": {
        "files": ["src/Support/helpers.php"],
        "psr-4": {"Elephant\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"Elephant\\Tests\\": "tests/"}
    }
}
